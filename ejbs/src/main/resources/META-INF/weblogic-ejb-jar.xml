<?xml version = '1.0' encoding = 'UTF-8'?>
<weblogic-ejb-jar>
    <weblogic-enterprise-bean>
        <ejb-name>HashOperation</ejb-name>
        <stateless-session-descriptor>
            <pool>
                <max-beans-in-free-pool>500</max-beans-in-free-pool>
                <initial-beans-in-free-pool>250</initial-beans-in-free-pool>
            </pool>
        </stateless-session-descriptor>
    </weblogic-enterprise-bean>

    <managed-scheduled-executor-service>
        <name>concurrent/CustomMSES</name>
        <dispatch-policy>qrs</dispatch-policy>
        <max-concurrent-long-running-requests>1</max-concurrent-long-running-requests>
        <long-running-priority>1</long-running-priority>
    </managed-scheduled-executor-service>


</weblogic-ejb-jar>